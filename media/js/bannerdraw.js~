			$(document).ready(function(){
				$("#banner3").fadeOut(500);
				$("#banner3").slideDown("slow");
				$(".bar3text").fadeOut(500);
				$(".bar3text").slideDown("slow");
				$(".bar3text").mouseenter(function() {
					$(".bar3text").fadeOut(1000);
					$(".bar3text").slideToggle("slow");
				});
				
			});